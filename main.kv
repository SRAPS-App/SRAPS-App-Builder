#:import webbrowser  webbrowser
#: import Window kivy.core.window.Window
#: import MDBoxLayout kivymd.uix.boxlayout.MDBoxLayout 
#: import dp kivy.metrics.dp

<BaseShadowWidget>
    size_hint: None, None
    size: 100, 100
    md_bg_color: 0, 0, 1, 1
    elevation: 36
    pos_hint: {'center_x': .5}	
    

<MyLayout@MDSwiperItem>:
	img:""
	FitImage:
        source: root.img
        radius: [20,]					
					
<headitem>
	h1:""
	MyMDCard:
		size_hint_y: None 
		size:(app.y-dp(20),"50dp")
		md_bg_color:1,1,1,1
		AnchorLayout:
			anchor_y:"top"
			anchor_x: 'left'
			MDLabel:
				text:root.h1
				font_name: 'assets/Poppins-Regular.ttf'
				size:"20sp"
<FitImgCard@AnchorLayout>
	text:""
	img:""
	link:""
	size:(app.y,"300dp")
	MyMDCard:
		radius:"40dp"
		elevation:30
		halign:"center"
		size_hint:None,None
		style:"elevated"
		size: app.y-app.y//17, "200dp"
		md_bg_color:app.theme_cls.accent_light		

		FitImage:
			source:root.img
			radius:"40dp"
		MDBoxLayout:
			orientation:"vertical"
			MDLabel:
				text:root.text
				halign:"center"
				font_name:"assets/Poppins-Bold.ttf"
				font_size:"25sp"

			AnchorLayout:
				orientation:"vertical"
				anchor_x:'center'
				anchor_y:'center'
				MDRoundFlatButton:
					text:"Read More"
					
					halign:"center"					
					line_width:"3dp"   		
					
    				size:(self.width,"30dp")
    				on_press:webbrowser.open(root.link)
<FitCard2@MyMDCard>
	text:""
	image:""
	MyMDCard:
		radius:"60dp"
		elevation:10
		halign:"center"
		size_hint:None,None
		size: app.y-app.y//13, "200dp"
		style:'outlined'
		line_color:0,0,0,1
		orientation:"horizontal"
		line_width:20
		md_bg_color:app.colorHex("#FFEFE6")
		FitImage:
			source:root.image
			radius:[30,10]
		MDLabel:
			text:root.text

		
<FitCard@MyMDCard>
	text:""
	MyMDCard:
		radius:"40dp"
		elevation:40
		halign:"center"
		size_hint:None,None
		size: app.y-app.y//13, "120dp"
		style:'elevated'
		md_bg_color:app.theme_cls.primary_light
		MDLabel:
			text:root.text
			halign:"center"
			font_name:"assets/Poppins-Bold.ttf"
			halign:'center'
<MyImg@MyMDCard>:
	radius:"60dp"
	elevation:40
	halign:"center"
	size_hint:None,None
	size: app.y-app.y//13,"170dp"
	style:'elevated'
	md_bg_color:app.colorHex("#B0FFB4")
	FitImage:
		id:nimg1
		source:app.NI
		radius:[30]
<Content>:
    orientation: "vertical"
    padding: dp(10)
    spacing: dp(10)
    height:(12)
    adaptive_height: True
	MDDataTable:
		size_hint:(0.99, 0.99)
		use_pagination:True
		check:True
		column_data:[("No.", dp(30)),("Name", dp(30)),("Designation", dp(60)),("Qualification", dp(30))]
		row_data:app.Teachers
<NavigationButton@Button_Item>
    icon_color: app.theme_cls.text_color
    text_color: app.theme_cls.text_color
    button_bg_color: app.theme_cls.primary_color
    mode: 'color_on_active'
    badge_disabled: True
<Card>:

