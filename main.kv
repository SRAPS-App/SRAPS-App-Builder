#:import webbrowser  webbrowser
#: import Window kivy.core.window
<FitImgCard@MDCard>
	text:""
	img:""
	link:""
	MDCard:
		radius:60
		elevation:10
		halign:"center"
		size_hint:None,None
		size: app.y-app.y//15, "200dp"
		style:'outline'
		line_color:0,0,0,1
		line_width:20
		md_bg_color:app.colorHex("#FFEFE6")
		FitImage:
			source:root.img
			radius:[30,10]
		MDBoxLayout:
			orientation:"vertical"
			padding:self.width//20
			MDLabel:
				#pos_hint:{"center_x":0.5,"centre_y":0.10}
				text:root.text
				halign:"center"
				font_name:"assets/Poppins-Bold.ttf"
				font_size:"25sp"
			

			MDRoundFlatButton:
				text:"Read More"
				text_color: 1, 0, 0, 1
				halign:"center"					
				line_width:5   		
    			line_color: 1, 0,0 , 1
    			on_press:webbrowser.open(root.link)
