#:import webbrowser  webbrowser
<FitImgCard@MDCard>
	text:""
	img:""
	link:""
	MDCard:
		radius:60
		elevation:10
		halign:"center"
		size_hint:None,None
		size: "340dp", "200dp"
		style:'outline'
		line_color:0,0,0,1
		line_width:20
		md_bg_color:app.colorHex("#FFEFE6")
		FitImage:
			source:root.img
			radius:[30,10]
		MDBoxLayout:
			orientation:"vertical"
			spacing:10
			MDLabel:
				pos_hint:{"center_x":0.5,"centre_y":0.10}
				text:root.text
				halign:"center"
				font_name:"assets/Poppins-Bold.ttf"
				font_size:"25sp"
			MDGridLayout:
				cols:1
				padding:120
				MDRoundFlatButton:
					text:"Read More"
					pos_hint:{"center_x":0.5,"centre_y":0.50}
					text_color: 1, 0, 0, 1
					halign:"center"
					line_width:5
    				line_color: 1, 0,0 , 1
    				on_press:webbrowser.open(root.link)

<FitCard@MDCard>
	text:""
	MDCard:
		radius:60
		elevation:40
		halign:"center"
		size_hint:None,None
		size: "340dp", "120dp"
		style:'elevated'
